# Default
server {
  listen 80;

  server_name miniserver01.local;

  location /absensi {
    include /etc/nginx/includes/proxy.conf;
    proxy_pass http://absensi;
  } 

  location /abs {
    proxy_pass http://absensiexit
;
 }

  location = /backend-not-found.html {
    allow   all;
  }

  location / {
    return 404;
  }


  access_log off;
  log_not_found off;
  error_log  /var/log/nginx/error.log error;
}
