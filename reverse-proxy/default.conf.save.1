# Default
server {
  listen 80;

  server_name miniserver01.local;

sis_auth_backend
  location /absensi {
    include /etc/nginx/includes/proxy.conf;
    proxy_pass http://absensi/;
  } 

  location /abs {
    proxy_pass http://absensi/;
 }

sis_auth_backend


  access_log off;
  log_not_found off;
  error_log  /var/log/nginx/error.log error;
}
